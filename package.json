{
  "name": "doc-check",
  "description": "Automatic Documentation Consistency Checker",
  "version": "0.0.0",
  "module": "index.ts",
  "type": "module",
  "author": "Alex Vukadinov",
  "homepage": "https://github.com/alexvuka1/doc-check",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexvuka1/doc-check.git"
  },
  "bugs": {
    "url": "https://github.com/alexvuka1/doc-check/issues"
  },
  "keywords": [
    "actions",
    "documentation",
    "consistency",
    "checker"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "bundle": "bun run format:write && bun run package",
    "format:write": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "bunx eslint .",
    "package": "ncc build src/index.ts --license licenses.txt",
    "package:watch": "bun run package -- --watch",
    "all": "bun run format:write && bun run lint && bun test && bun run package"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@apidevtools/swagger-parser": "^10.1.0",
    "@readme/openapi-parser": "^2.5.0",
    "ajv": "^8.12.0",
    "lodash-es": "^4.17.21",
    "marked": "^12.0.0",
    "mdast": "^3.0.0",
    "mdast-util-from-markdown": "^2.0.0",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "remark-sectionize": "^2.0.0",
    "to-vfile": "^8.0.0",
    "unified": "^11.0.4",
    "unist-util-filter": "^5.0.1",
    "unist-util-find-after": "^5.0.0",
    "unist-util-map": "^4.0.0",
    "unist-util-select": "^5.1.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-children": "^3.0.0",
    "unist-util-visit-parents": "^6.0.1"
  },
  "devDependencies": {
    "@types/bun": "^1.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-prettier": "^5.1.3",
    "openapi-types": "^12.1.3",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "type-fest": "^4.11.1",
    "typescript": "^5.3.3"
  }
}
